--exclude=.*
--exclude=build
--exclude=dist
--exclude=venv
--exclude=env
--exclude=node_modules

--tag-relative=yes

--python-kinds=-iv

--make-kinds=-m

--langdef=Go
--langmap=Go:.go
--regex-Go=/func([ \t]+\([^)]+\))?[ \t]+([a-zA-Z0-9_]+)/\2/f,func/
--regex-Go=/var[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/v,var/
--regex-Go=/type[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/t,type/
