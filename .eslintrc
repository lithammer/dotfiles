{
    "extends": "eslint:recommended",
    "rules": {
        "indent": [2, 4],
        "quotes": [1, "double"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "no-console": 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ]
}
